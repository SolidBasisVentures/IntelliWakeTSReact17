{
	"name": "@solidbasisventures/intelliwaketsreact17",
	"version": "4.1.27",
	"description": "NPM package for IntelliWake React 17 Components",
	"type": "module",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.umd.cjs"
		}
	},
	"scripts": {
		"Vitest-Watch": "vitest",
		"TSNodeDev": "ts-node-dev --script-mode --respawn --transpile-only consoles/consoles.ts",
		"Install-IntelliWake": "pnpm install && pnpm add -D @solidbasisventures/intelliwaketsfoundation@latest",
		"Version-Minor-Advance": "pnpm version minor",
		"Publish": "tsc && vite build && pnpm version patch && git push --tags && git push && npm publish",
		"RollupBuildAndPublish": "rollup -c && npm version patch --no-git-tag-version && npm publish"
	},
	"files": [
		"dist",
		"dist/*.scss"
	],
	"author": "SolidBasisVentures",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SolidBasisVentures/IntelliWakeTSReact17.git"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.2.1",
		"@fortawesome/pro-duotone-svg-icons": "6.2.1",
		"@fortawesome/pro-light-svg-icons": "6.2.1",
		"@fortawesome/pro-regular-svg-icons": "6.2.1",
		"@fortawesome/pro-solid-svg-icons": "6.2.1",
		"@fortawesome/react-fontawesome": "^0.1.18",
		"@popperjs/core": "^2.4.0",
		"@solidbasisventures/intelliwaketsfoundation": "^5.11.13",
		"@types/cleave.js": "^1.4.2",
		"@types/node": "^18.18.13",
		"@types/qrcode.react": "^1.0.1",
		"@types/react": "^17.0.75",
		"@types/react-dom": "^17.0.25",
		"@types/react-router-dom": "^5.3.3",
		"axios": "^1.6.2",
		"bootstrap": "^5.2.2",
		"cleave.js": "^1.6.0",
		"prettier": "^2.8.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.3.4",
		"react-switch": "^6.0.0",
		"recoil": "^0.7.3",
		"sass": "^1.69.5",
		"source-map-support": "^0.5.21",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tslib": "^2.6.2",
		"typescript": "^4.9.5",
		"vite": "^5.0.12",
		"vite-plugin-dts": "^3.6.3",
		"vite-plugin-static-copy": "^1.0.0",
		"vitest": "^0.34.6"
	}
}
